{
	"name": "form-data-serializer",
	"version": "0.1.3",
	"description": "A modern form data serializer with extension support",
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"sideEffects": false,
	"engines": {
		"node": ">=18.0.0"
	},
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./extensions": {
			"import": "./dist/extensions/index.js",
			"types": "./dist/extensions/index.d.ts"
		}
	},
	"typesVersions": {
		"*": {
			"extensions": [
				"./dist/extensions/index.d.ts"
			]
		}
	},
	"devDependencies": {
		"@faker-js/faker": "^10.0.0",
		"@playwright/test": "^1.55.1",
		"@types/bun": "latest",
		"playwright": "^1.55.1",
		"typedoc": "^0.28.13",
		"typedoc-plugin-markdown": "^4.9.0"
	},
	"peerDependencies": {
		"typescript": "^5.9.3"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"test": "bun test --root test/unit --coverage",
		"test:integration": "bunx playwright test --config src/test/integration/next.js/playwright.config.ts",
		"lint": "bunx biome lint",
		"format": "bunx biome format --write",
		"build": "bun run src/build.ts"
	},
	"dependencies": {
		"uuid": "^13.0.0"
	}
}
